package uberhub;
import java.util.Scanner;

public class uri1132 {

	public static void main(String[] args) {
		Scanner leitor = new Scanner(System.in);
		
		int soma = 0;
		int X = leitor.nextInt();
		int Y = leitor.nextInt();
		
		if (X <= Y) {
			for (int i = X; i <= Y; i++) {
				if (i % 13 != 0) {
					soma = soma + i;
				}
			}			
		} else {
			for (int i = Y; i <= X; i++) {
				if (i % 13 != 0) {
					soma = soma + i;
				}
			}
		}
		
		
		System.out.println(soma);

	}

}
