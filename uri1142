package uberhub;
import java.util.Scanner;

public class uri1142 {

	public static void main(String[] args) {
		Scanner leitor = new Scanner(System.in);
		
		int N = leitor.nextInt();
		
		for (int i = 1; i <= (N * 4); i++) {
			if (i % 4 == 0) {
				System.out.print("PUM" + "\n");				
			} else {
				System.out.print(i + " ");
			}
			
		}
		
	}

}
