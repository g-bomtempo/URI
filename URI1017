package uberhub;
import java.util.Scanner;

public class uri1017 {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		
		int x = ler.nextInt();
		int y = ler.nextInt();
		
		double total = (y/12.0) * x;
		
		System.out.println(String.format("%.3f", total));
		
	}

}
