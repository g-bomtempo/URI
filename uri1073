package uberhub;
import java.util.Scanner;

public class uri1073 {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		
		int leitura = ler.nextInt();
		
		
		for (int i = 1; i <= leitura; i++) {
			if (i % 2 == 0) {
				System.out.println(i + "^2 = " + (i*i));
			}
		}
	}
		
	

}
