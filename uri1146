package uberhub;
import java.util.Scanner;

public class uri1146 {

	public static void main(String[] args) {
		Scanner leitor = new Scanner(System.in);
		
		while(true) {
			int X = leitor.nextInt();			
			
			if(X == 0) {
				break;
			}
			
			for (int i = 1; i<= X; i++) {
				if (i != X) {
					System.out.print(i + " ");
				} else {
					System.out.print(i + "\n");
				}				
					
			}
			
		}		

	}
}
